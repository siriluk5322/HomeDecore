<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dip" />
            <stroke android:width="1dip" android:color="#C4C4C6" />
            <gradient android:startColor="#C6C4C4" android:endColor="#C6C4C4"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="5dip" />
            <stroke android:width="1dip" android:color="#C6C4C4" />
            <gradient android:startColor="#C6C4C4" android:endColor="#C6C4C4"/>

        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dip" />
            <stroke android:width="1dip" android:color="#FFC6C4C4" />
            <gradient android:startColor="#FFC6C4C4" android:endColor="#FFC6C4C4"/>
        </shape>
    </item>
</selector>